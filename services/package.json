{
  "name": "ticket-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "db:all": "yarn db:migrate:undo && yarn db:migrate && yarn db:seed",
    "db:migrate": "set NODE_ENV=development&&yarn m:sequelize db:migrate",
    "db:migrate:undo": "yarn m:sequelize db:migrate:undo:all",
    "db:seed": "yarn m:sequelize db:seed:all",
    "db:seed:undo": "yarn m:sequelize db:seed:undo:all",
    "m:sequelize": "./node_modules/.bin/sequelize --config database/config.js --models-path database/models/ --migrations-path database/migrations/ --seeders-path database/seeders"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "lodash": "^4.17.15",
    "morgan": "~1.9.0",
    "mysql2": "~1.5.2",
    "nodemon": "^2.0.2",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^5.5.1",
    "winston": "^2.4.2"
  }
}
